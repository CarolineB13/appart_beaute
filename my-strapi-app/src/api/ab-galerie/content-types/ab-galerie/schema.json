{
  "kind": "collectionType",
  "collectionName": "ab_galeries",
  "info": {
    "singularName": "ab-galerie",
    "pluralName": "ab-galeries",
    "displayName": "ABGalerie"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "categorie": {
      "type": "enumeration",
      "enum": [
        "Onglerie",
        "Épilation",
        "Cils",
        "Avant/Après",
        "Autre"
      ],
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "publie": {
      "type": "boolean",
      "default": true,
      "required": true
    }
  }
}
